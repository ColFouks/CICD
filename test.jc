imports = ["default.cfg", "build.cfg"]
common {
    github {
        org = "SomeOrg"
        repo = "JavaTest"
    }
    folder  {
        project = "Java"
    }
}

__build {
    imports = ["maven.cfg"]
    maven {
        steps = "deploy"
    }
}
__pipeline {
    folder {
        jobType = "Pipeline"
    }
}
Build {
    imports = ["__build"]
}
Pipeline {
    imports = ["__pipeline"]
    jobClass {
        baseClassName = "Pipeline"
        classPath = ""
    }    
    pipeline = [ Build: [
            BRANCH_NAME:"master"
        ]
    ]
}
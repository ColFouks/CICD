imports = ["globalConfig.cfg", "buildConfig.cfg"]
common {
    github {
        org = "SomeOrg"
        repo = "JavaTest"
    }
    folder  {
        project = "Java"
    }
}

__build {
    imports = ["mavenBuild.cfg"]
    maven {
        steps = "deploy"
    }
}
__pipeline {
    folder {
        jobType = "Pipeline"
    }
}
Build {
    imports = ["__build"]
}
Pipeline {
    imports = ["__pipeline"]
    jobClass {
        baseClassName = "Pipeline"
        classPath = ""
    }    
}